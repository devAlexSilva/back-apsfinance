generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Deposit {
  id    String @id @default(uuid())
  price Int 
  info String

  @@map("deposits")
}

model Withdrawn {
  id    String @id @default(uuid())
  price Int 
  info String

  @@map("withdrawns")
}